{
  "name": "@dialectlabs/workspaces",
  "version": "0.1.3",
  "description": "",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dialectlabs/react"
  },
  "scripts": {
    "bootstrap": "yarn build:all",
    "build:all": "yarn run build:react || yarn run build:react-ui",
    "build:react-ui": "yarn workspace @dialectlabs/react-ui run build",
    "build:react": "yarn workspace @dialectlabs/react run build",
    "dev:react-ui": "yarn workspace @dialectlabs/react-ui run dev",
    "dev:react": "yarn workspace @dialectlabs/react run dev",
    "lint": "eslint --ext .js --ext .ts --ext .tsx .",
    "lint:fix": "yarn lint --fix"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "concurrently": "^7.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.4.0",
    "typescript": "^4.5.5"
  },
  "keywords": [],
  "author": "@dialectlabs",
  "license": "Apache-2.0",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "workspaces": [
    "examples/*",
    "packages/*",
    "starters/*"
  ],
  "engines": {
    "node": ">=15.10 <17"
  },
  "engineStrict": true
}
